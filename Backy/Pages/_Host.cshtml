@page "/"
@namespace Backy.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Backy.Services
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Backy</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="css/site.css" rel="stylesheet" />
    <link href="css/FileExplorer.css" rel="stylesheet" />
    <link href="css/Index.css" rel="stylesheet" />
    <link href="css/Drive.css" rel="stylesheet" />
    <link href="css/DarkMode.css" rel="stylesheet" />
    <link href="_content/Blazored.Toast/blazored-toast.css" rel="stylesheet" />
    <HeadOutlet />
</head>

<body>
    <app>
        <component type="typeof(Backy.App)" render-mode="ServerPrerendered" />
    </app>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="_content/Blazored.Toast/blazored-toast.min.js"></script>
</body>

</html>
